macro_app_2: macro_app_2{
compatible = "zmk,behavior-macro";
label = "macro_app_2";
#binding-cells = <0>;
bindings
    = <&macro_press &kp LEFT_CONTROL>
    , <&macro_press &kp LEFT_COMMAND>
    , <&macro_tap &kp N2>
    , <&macro_release &kp LEFT_CONTROL>
    , <&macro_release &kp LEFT_COMMAND>
    ;
};

macro_app_1: macro_app_1{
compatible = "zmk,behavior-macro";
label = "macro_app_1";
#binding-cells = <0>;
bindings
    = <&macro_press &kp LEFT_CONTROL>
    , <&macro_press &kp LEFT_COMMAND>
    , <&macro_tap &kp N1>
    , <&macro_release &kp LEFT_CONTROL>
    , <&macro_release &kp LEFT_COMMAND>
    ;
};

macro_apps: macro_apps{
compatible = "zmk,behavior-macro";
label = "macro_apps";
#binding-cells = <0>;
bindings
    = <&macro_param_1to1>
    , <&macro_press &kp LEFT_CONTROL>
    , <&macro_press &kp LEFT_COMMAND>
    , <&macro_tap &kp MACRO_PLACEHOLDER>
    , <&macro_release &kp LEFT_CONTROL>
    , <&macro_release &kp LEFT_COMMAND>
    ;
};

macro_app: macro_app{
compatible = "zmk,behavior-macro";
label = "macro_app";
#binding-cells = <0>;
bindings
    = <&macro_press &mo 1 &kp LEFT_CONTROL>
    , <&macro_press &mo 1 &kp LEFT_COMMAND>
    , <&macro_pause_for_release>
    , <&macro_release &mo 1 &kp LEFT_CONTROL>
    , <&macro_release &mo 1 &kp LEFT_COMMAND>
    ;
};

macro_kinesis: macro_kinesis{
compatible = "zmk,behavior-macro";
label = "macro_kinesis";
#binding-cells = <0>;
bindings = <&kp LS(K)>, <&kp I>, <&kp N>, <&kp E>, <&kp S>, <&kp I>, <&kp S>;
};
