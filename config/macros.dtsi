macro_app_4: macro_app_4{
compatible = "zmk,behavior-macro";
label = "macro_app_4";
#binding-cells = <0>;
bindings = <&kp LEFT_CONTROL>, <&kp LEFT_COMMAND>, <&kp N4>;
};
macro_app_3: macro_app_3{
compatible = "zmk,behavior-macro";
label = "macro_app_3";
#binding-cells = <0>;
bindings = <&kp LEFT_CONTROL>, <&kp LEFT_COMMAND>, <&kp N3>;
};

macro_app_2: macro_app_2{
compatible = "zmk,behavior-macro";
label = "macro_app_2";
#binding-cells = <0>;
bindings
    = <&macro_press &kp LEFT_CONTROL>
    , <&macro_press &kp LEFT_COMMAND>
    , <&kp N2>
    , <&macro_release &kp LEFT_CONTROL>
    , <&macro_release &kp LEFT_COMMAND>
    ;
};

macro_app_1: macro_app_1{
compatible = "zmk,behavior-macro";
label = "macro_app_1";
#binding-cells = <0>;
bindings
    = <&macro_press &kp LEFT_CONTROL>
    , <&macro_press &kp LEFT_COMMAND>
    , <&macro_tap &kp N1>
    , <&macro_release &kp LEFT_CONTROL>
    , <&macro_release &kp LEFT_COMMAND>
    ;
};

macro_kinesis: macro_kinesis{
compatible = "zmk,behavior-macro";
label = "macro_kinesis";
#binding-cells = <0>;
bindings = <&kp LS(K)>, <&kp I>, <&kp N>, <&kp E>, <&kp S>, <&kp I>, <&kp S>;
};
